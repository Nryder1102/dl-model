{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
            "components/*": ["src/SceneController/components/*"],
            "hook/*": ["src/SceneController/hook/*"],
            "data/*": ["src/data/*"]
        },
        "checkJs": true,
        "target": "es2022",
        "lib": ["es2022", "DOM", "webworker"],
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "module": "es2020"
    }
}
